program soal4;
uses crt;

var
    jalan, no, kota, kodepos : string;

begin
    clrscr;
    write('Jalan : ');
    readln(jalan);
    write('No : ');
    readln(no);
    write('Kota : ');
    readln(kota);
    write('Kode Pos : ');
    readln(kodepos);

    writeln('Alamat: Jalan ',jalan,' No. ',no,' ',kota,' ',kodepos );
end.
